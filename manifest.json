{
    "manifest_version": 3,
    "name": "Mute marketing signals",
    "version": "1.1",
    "permissions": ["activeTab", "scripting", "storage"],
    "content_scripts": [
      {
        "matches": ["https://*.rsaweb.co.za/*"],
        "js": ["mute-marketing.js"],
        "run_at": "document_start"
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }  