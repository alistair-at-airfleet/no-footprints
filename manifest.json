{
    "manifest_version": 3,
    "name": "No footprints",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"]
    },
    "content_scripts": [
      {
        "matches": ["*://*.augury.com/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  