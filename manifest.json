{
    "manifest_version": 3,
    "name": "No footprints",
    "version": "1.1",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://*.augury.com/*"],
        "js": ["footprint.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }  